                "waitOnAll": true
              },
              "transforms": [
                {
                  "code": "\t\n\t\n\tdbName \u003d value[\u0027dbName\u0027]\n\tdbType \u003d value[\u0027dbType\u0027]\n\ttableName \u003d value[\u0027tableName\u0027]\n\n\tparams \u003d { \u0027database\u0027:dbName, \u0027tableName\u0027:tableName }\n\tqueryPath \u003d \"General/\" +  dbType + \"/getTable\"\n\t\n\ttry:\n\t\tresults \u003d system.db.runNamedQuery(queryPath, params)\n\texcept:\n\t\tresults \u003d []\n\t\n\treturn results\n",
                  "type": "script"
                }
              ],
              "type": "expr-struct"
