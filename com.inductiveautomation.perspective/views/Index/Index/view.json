              {
                "events": {
                  "component": {
                    "onActionPerformed": {
                      "config": {
                        "script": "\t\n\timport os\n\timport time\n\t\n\tinstallationPath \u003d self.parent.getChild(\"Ignition Path\").getChild(\"TextField\").props.text\n\tprojectName \u003d system.project.getProjectName()\n\tresourcesPath \u003d installationPath + \u0027/Ignition/data/projects/\u0027 + projectName + \u0027/resources\u0027\n\trestoreTypes \u003d self.parent.getChild(\"File Upload\").getChild(\"FileUpload\").custom.resourceFileContents\n\n\n\tfor restoreType in restoreTypes: \n\t\t\n\t\t# ------------ Tags -------------------------------------------\t\t\n\t\tif restoreType[\u0027type\u0027] \u003d\u003d \u0027tags\u0027 and not self.parent.custom.tagsExist:\n\n\t\t\toptions \u003d restoreType[\u0027options\u0027]\n\t\t\n\t\t\tfor option in options:\n\t\t\t\tif \u0027RapidDev\u0027 in option[\u0027fileName\u0027]:\n\t\t\t\t\tbackup.restore.restoreTags(resourcesPath, option[\u0027fileName\u0027])\n\n\n\t\t# ------------ Themes --------------------------------------------------------\n\t\tif restoreType[\u0027type\u0027] \u003d\u003d \u0027themes\u0027 and not self.parent.custom.themesExist:\t\t\n\n\t\t\toptions \u003d restoreType[\u0027options\u0027]\n\t\t\tfor option in options:\n\t\t\t\tif \u0027rapid-dev\u0027 in option[\u0027label\u0027]:\n\t\t\t\t\tbackup.restore.restoreThemes(installationPath, resourcesPath, option[\u0027label\u0027])\n\n\t\t\n\t\t\n\t\t# ------------ Icons --------------------------------------------------------\t\t\t\t\n\t\tif restoreType[\u0027type\u0027] \u003d\u003d \u0027icons\u0027 and not self.parent.custom.iconsExist:\t\t\n\n\t\t\toptions \u003d restoreType[\u0027options\u0027]\n\n\t\t\tfor option in options:\n\t\t\t\tif \u0027rapid-dev\u0027 in option[\u0027fileName\u0027]:\n\t\t\t\t\tbackup.restore.restoreIcons(installationPath, resourcesPath, option[\u0027fileName\u0027])\n\n\n\n\n\t# update settings installationPathIA from user imput\n\tif installationPath:\n\t\ttagPath \u003d settings.ROOT_PATH + \u0027/RapidDev/Global/Settings/installationPathIA\u0027\n\t\tif system.tag.exists(tagPath):\n\t\t\tsystem.tag.writeBlocking(tagPath, installationPath)\n\t\n\t\n\t# if version \u003e 8.1.28 update immediately\n\ttry:\n\t\tsystem.project.requestScan()\n\texcept:\n\t\tpass\n\t\t\n\t\t\n\n\t# flash startup banner - I think I was drunk when I did this.\n\tself.parent.position.display \u003d False\n\tself.parent.parent.getChild(\"Startup\").position.display \u003d True\n\ttime.sleep(4)\n\n\n\t# navigate to the first page\n\tmenuDS \u003d settings.getValue(\"Global\", \"menu\")\n\n\tfor row in range(menuDS.getRowCount()):\n\t\torder \u003d menuDS.getValueAt(row,\"order\")\n\t\tif row \u003d\u003d 0:\n\t\t\tminOrder \u003d order\n\t\t\trootRow \u003d row\n\t\telif order \u003c minOrder:\n\t\t\tminOrder \u003d order\n\t\t\trootRow \u003d row\n\t\n\trootURL \u003d menuDS.getValueAt(rootRow, \"pageURL\")\n\t\n\tsystem.perspective.navigate(rootURL[1:])\n\n"
                      },
                      "scope": "G",
                      "type": "script"
                    }
                  }
